        <!-- "new" PROTEUS questions come in two flavors                           -->
        <!--     "proteus" - used for the study, approx 3 x 11 = 33                -->
        <!--     "proteus-unused" - authored, added, but not elected               -->
        <!-- "original" FCLA Reading Questions come in two flavors                 -->
        <!--     "fcla-rq" - used in the rest of the book in study                 -->
        <!--     "fcla-rq-non-proteus" - not visible in study due to new questions -->
        <version include="early proteus fcla-rq"/>
        <directories external="ext" generated="gen"/>
